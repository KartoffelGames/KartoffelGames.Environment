{
    "name": "@kartoffelgames/environment",
    "version": "3.0.4",
    "license": "LGPL-3.0-only",
    "tasks": {},
    "imports": {
        "@zip-js/zip-js": "jsr:@zip-js/zip-js@^2.7.57"
    },
    "exports": {
        ".": "./source/index.ts"
    },
    "publish": {
        "include": [
            "source/",
            "blueprint/",
            "deno.json",
            "LICENSE"
        ]
    },
    "kg": {
        "name": "Kartoffelgames.Environment",
        "config": {
            "blueprint": "kg-main",
            "pack": false,
            "page": {
                "enabled": false,
                "mainBundleRequired": false,
                "port": 8088
            },
            "package-blueprint": "",
            "bundle": {
                "enabled": false,
                "bundleSettingsFile": ""
            },
            "scratchpad": {
                "mainBundleRequired": false,
                "port": 8088
            },
            "test": {
                "bundleRequired": false,
                "directory": "./test"
            },
            "transform": {
                "enableNode": false,
                "nodeDirectory": "./node"
            }
        },
        "source": "./source"
    }
}