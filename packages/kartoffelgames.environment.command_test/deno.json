{
    "name": "@kartoffelgames/environment-command-test",
    "version": "3.2.2",
    "license": "LGPL-3.0-only",
    "tasks": {},
    "imports": {
        "@std/testing": "jsr:@std/testing@^1.0.9",
        "@std/expect": "jsr:@std/expect@^1.0.13"
    },
    "exports": {
        ".": "./source/index.ts",
        "./kg-cli.config.json": "./kg-cli.config.json"
    },
    "publish": {
        "include": [
            "source/",
            "deno.json",
            "kg-cli.config.json",
            "LICENSE"
        ]
    },
    "kg": {
        "name": "Kartoffelgames.Environment.Command_Test",
        "config": {
            "blueprint": "kg-main",
            "bundle": {
                "enabled": false,
                "bundleSettingsFile": ""
            },
            "page": {
                "enabled": false,
                "mainBundleRequired": false,
                "port": 8088
            },
            "scratchpad": {
                "mainBundleRequired": false,
                "port": 8888
            },
            "test": {
                "bundleRequired": false,
                "directory": "./test"
            },
            "package-blueprint": "",
            "transform": {
                "enableNode": false,
                "nodeDirectory": "./node"
            }
        },
        "source": "./source"
    }
}