{
    "name": "@kartoffelgames/environment-bundle",
    "version": "4.0.1",
    "license": "LGPL-3.0-only",
    "tasks": {},
    "imports": {
        "esbuild": "npm:esbuild@^0.25.2",
        "@oazmi/esbuild-plugin-deno": "jsr:@oazmi/esbuild-plugin-deno@^0.4.1"
    },
    "exports": "./source/index.ts",
    "publish": {
        "include": [
            "source/",
            "deno.json",
            "LICENSE"
        ]
    },
    "kg": {
        "name": "Kartoffelgames.Environment.Bundle",
        "config": {
            "package_blueprint": "kg-main",
            "tests": [
                "unit"
            ],
            "page": {
                "enabled": false,
                "mainBundleRequired": false,
                "port": 8088,
                "bundleSettingsFile": ""
            },
            "bundle": {
                "enabled": false,
                "bundleSettingsFile": ""
            },
            "package-blueprint": "",
            "scratchpad": {
                "mainBundleRequired": false,
                "port": 8088,
                "bundleSettingsFile": ""
            },
            "test": {
                "bundleRequired": false,
                "directory": "./test"
            },
            "transform": {
                "enableNode": false,
                "nodeDirectory": "./node"
            }
        },
        "source": "./source"
    }
}