{
    "name": "@kartoffelgames/environment-test-package",
    "version": "4.0.1",
    "license": "LGPL-3.0-only",
    "tasks": {},
    "imports": {
        "@std/testing": "jsr:@std/testing@^1.0.9",
        "@std/expect": "jsr:@std/expect@^1.0.13"
    },
    "exports": {
        ".": "./source/index.ts"
    },
    "publish": {
        "include": [
            "source/",
            "deno.json",
            "LICENSE"
        ]
    },
    "kg": {
        "name": "Kartoffelgames.Environment.Test_Package",
        "config": {
            "package-blueprint": "kg-main",
            "bundle": {
                "enabled": true,
                "bundleSettingsFile": "./bundle-environment/bundle-settings.ts"
            },
            "scratchpad": {
                "mainBundleRequired": false,
                "port": 8088,
                "bundleSettingsFile": "./bundle-environment/bundle-settings.ts"
            },
            "page": {
                "enabled": true,
                "mainBundleRequired": true,
                "port": 8088,
                "bundleSettingsFile": "./bundle-environment/bundle-settings.ts"
            },
            "test": {
                "directory": "./test",
                "bundleRequired": false
            },
            "transform": {
                "enableNode": false,
                "nodeDirectory": "./node"
            }
        },
        "source": "./source"
    }
}